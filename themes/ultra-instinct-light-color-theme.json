  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#64748B",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#0F766E"
      }
    },
    {
      "scope": ["keyword", "storage", "storage.type", "keyword.control"],
      "settings": {
        "foreground": "#7E22CE",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#334155",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "variable",
        "variable.other.readwrite",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#0F766E"
      }
    },
    {
      "scope": [
        "string",
        "string.quoted",
        "string.template",
        "constant.other.color",
        "support.constant.color",
        "support.constant.property-value",
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#0369A1"
      }
    },
    {
      "scope": ["constant.character.escape", "constant.character"],
      "settings": {
        "foreground": "#7E22CE"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "support.class",
        "support.type",
        "entity.name.class",
        "new.expr entity.name.type"
      ],
      "settings": {
        "foreground": "#334155",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#7E22CE"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#0F766E",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "meta.selector.css",
        "entity.name.tag.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#7E22CE"
      }
    },
    {
      "scope": ["entity.other.attribute-name.class.css"],
      "settings": {
        "foreground": "#7E22CE",
        "fontStyle": "bold italic"
      }
    },
    {
      "scope": ["support.type.property-name.css", "meta.property-name.css"],
      "settings": {
        "foreground": "#0F766E"
      }
    },
    {
      "scope": [
        "punctuation.section.braces",
        "punctuation.section.brackets",
        "meta.brace.curly",
        "meta.brace.square",
        "punctuation.section.property-list",
        "punctuation.definition.block",
        "punctuation.definition.parameters"
      ],
      "settings": {
        "foreground": "#64748B"
      }
    },
    {
      "scope": [
        "variable.language",
        "variable.language.this",
        "variable.language.self"
      ],
      "settings": {
        "foreground": "#7E22CE",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "meta.decorator",
        "meta.annotation",
        "punctuation.definition.annotation"
      ],
      "settings": {
        "foreground": "#4C1D95",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#0F766E"
      }
    },
    {
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#7E22CE",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold", "punctuation.definition.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic", "punctuation.definition.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.inline.raw"],
      "settings": {
        "foreground": "#0369A1"
      }
    },
    {
      "scope": ["markup.link.label", "string.other.link"],
      "settings": {
        "foreground": "#0F766E"
      }
    },
    {
      "scope": [
        "punctuation.section.braces",
        "punctuation.section.brackets",
        "punctuation.section.parens",
        "meta.brace.curly",
        "meta.brace.square",
        "meta.brace.round",
        "punctuation.definition.parameters",
        "punctuation.section.property-list",
        "punctuation.definition.block",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#64748B"
      }
    }
  ]
}
