  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#7878A0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#90A0FF"
      }
    },
    {
      "scope": ["keyword", "storage", "storage.type", "keyword.control"],
      "settings": {
        "foreground": "#448AFF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#DCE0E5",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "variable",
        "variable.other.readwrite",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#32bfcc"
      }
    },
    {
      "scope": [
        "string",
        "string.quoted",
        "string.template",
        "constant.other.color",
        "support.constant.color",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#E0E0FF"
      }
    },
    {
      "scope": [
        "punctuation.definition.string",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "punctuation.definition.string.template.begin",
        "punctuation.definition.string.template.end"
      ],
      "settings": {
        "foreground": "#485880"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#2979FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["constant.character.escape", "constant.character"],
      "settings": {
        "foreground": "#448AFF"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "support.class",
        "support.type",
        "entity.name.class",
        "new.expr entity.name.type"
      ],
      "settings": {
        "foreground": "#8C9EFF",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#536DFE"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#00EAFF",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "meta.selector.css",
        "entity.name.tag.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#536DFE"
      }
    },
    {
      "scope": ["entity.other.attribute-name.class.css"],
      "settings": {
        "foreground": "#448AFF",
        "fontStyle": "bold italic"
      }
    },
    {
      "scope": ["support.type.property-name.css", "meta.property-name.css"],
      "settings": {
        "foreground": "#00EAFF"
      }
    },
    {
      "scope": [
        "punctuation.section.braces",
        "punctuation.section.brackets",
        "meta.brace.curly",
        "meta.brace.square",
        "punctuation.section.property-list",
        "punctuation.definition.block",
        "punctuation.definition.parameters"
      ],
      "settings": {
        "foreground": "#FF00FF"
      }
    },
    {
      "scope": [
        "variable.language",
        "variable.language.this",
        "variable.language.self"
      ],
      "settings": {
        "foreground": "#BB88FF",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "meta.decorator",
        "meta.annotation",
        "punctuation.definition.annotation"
      ],
      "settings": {
        "foreground": "#7B42FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#40C4FF"
      }
    },
    {
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#DCE0E5",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold", "punctuation.definition.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic", "punctuation.definition.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.inline.raw"],
      "settings": {
        "foreground": "#F0F5FF"
      }
    },
    {
      "scope": ["markup.link.label", "string.other.link"],
      "settings": {
        "foreground": "#00EAFF"
      }
    },
    {
      "scope": [
        "punctuation.section.braces",
        "punctuation.section.brackets",
        "punctuation.section.parens",
        "meta.brace.curly",
        "meta.brace.square",
        "meta.brace.round",
        "punctuation.definition.parameters",
        "punctuation.section.property-list",
        "punctuation.definition.block",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#606b85"
      }
    }
